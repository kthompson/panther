using System

namespace Panther

object Predef {
    def print(object value): unit => System.Console.Write(value)

    def println(object value): unit => System.Console.WriteLine(value)

    def println(): unit => System.Console.WriteLine()

    def readLine(): string => System.Console.ReadLine()

    val _random = new System.Random()

    def rnd(max: int): int => _random.Next(max)
}
